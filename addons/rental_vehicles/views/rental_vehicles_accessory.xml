<odoo>
    <record id="view_accessory_list" model="ir.ui.view">
        <field name="name">accessory.list</field>
        <field name="model">rental_vehicles.accessory</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="name"/>
                <field name="office_id"/>
                <field name="default_price"/>
                <field name="currency_id"/>
                <field name="affects_salary"/>
            </list>
        </field>
    </record>

    <record id="view_accessory_form" model="ir.ui.view">
        <field name="name">accessory.form</field>
        <field name="model">rental_vehicles.accessory</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="office_id"/>
                        <field name="default_price"/>
                        <field name="currency_id"/>
                        <field name="affects_salary"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_accessory" model="ir.actions.act_window">
        <field name="name">accessory</field>
        <field name="res_model">rental_vehicles.accessory</field>
        <!-- <field name="path"></field> -->
        <!-- <field name="search_view_id" ref=""/> -->
        <!-- <field name="context">{
            'search_default_': 1,
        }</field> -->
        <field name="view_mode">list,form</field>
    </record>
</odoo>