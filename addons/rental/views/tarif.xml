<odoo>
    <record id="rental_tarif_form" model="ir.ui.view">
        <field name="name">rental.tarif.form</field>
        <field name="model">rental.tarif</field>
        <field name="arch" type="xml">
            <form string="Tarif">
                <sheet>
                    <group>
                        <field name="office_id" options="{'no_create': False}"/>
                        <field name="vehicle_type_id"/>
                        <field name="price"/>
                        <!-- <field name="currency_id" readonly="1"/> -->
                        <field name="currency_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="rental_tarif_list" model="ir.ui.view">
        <field name="name">rental.tarif.tree</field>
        <field name="model">rental.tarif</field>
        <field name="arch" type="xml">
            <list>
                <field name="office_id"/>
                <field name="vehicle_type_id"/>
                <field name="price"/>
                <field name="currency_id"/>
            </list>
        </field>
    </record>

    <record id="rental_tarif_window" model="ir.actions.act_window">
        <field name="name">Tarif</field>
        <field name="res_model">rental.tarif</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>
