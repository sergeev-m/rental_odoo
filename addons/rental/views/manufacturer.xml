<odoo>
    <record id="rental.manufacturer_form" model="ir.ui.view">
        <field name="name">rental.manufacturer.form</field>
        <field name="model">rental.manufacturer</field>
        <field name="arch" type="xml">
            <form string="Manufacturer">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="rental.manufacturer_list" model="ir.ui.view">
        <field name="name">rental.manufacturer.list</field>
        <field name="model">rental.manufacturer</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="rental.manufacturer_window" model="ir.actions.act_window">
        <field name="name">Manufacturer</field>
        <field name="res_model">rental.manufacturer</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>
